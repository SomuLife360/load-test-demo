          <stringProp name="BeanShellSampler.query">import java.io.IOException;
import java.io.FileWriter;
import java.io.File;
import java.io.BufferedWriter;
import java.time.Instant;
import org.apache.jmeter.services.FileServer;

String phoneNumbers = &quot;/Users/somu/Desktop/cloud-api-load-tests-develop/input/numbers.csv&quot;;
String usersFile = &quot;/Users/somu/Desktop/cloud-api-load-tests-develop/input/users.csv&quot;;
String header = &quot;phone,email&quot;;
StringBuilder sb  = new StringBuilder();
StringBuilder sb1  = new StringBuilder();

try {

//Generates a range of telephone numbers to create users for a given area-code.
		long startNum = 866555;
		for ( int i = 101; i&lt;= 101; i++ ) {

			sb1.append((startNum*10000)+i).append(&quot;\n&quot;);
		}
		FileWriter fw = new FileWriter(phoneNumbers,false);
		BufferedWriter bw = new BufferedWriter(fw);		
		bw.write(sb1.toString());
		bw.flush();
		fw.flush();
		bw.close();

//Generates a unique email address based on epoch time-stamp for user creation.
		bufferedReader = new BufferedReader(new FileReader(phoneNumbers));
		while (( line = bufferedReader.readLine()) != null ) {
			sb.append(line).append(&quot;,load-tests-&quot;).append(UUID.randomUUID().toString()).append(&quot;@life360-denali-automation.com&quot;).append(&quot;\n&quot;);
		}
		FileWriter fstream = new FileWriter(usersFile,false);
		BufferedWriter bufferedWriter = new BufferedWriter(fstream);
		bufferedWriter.write(header);
		bufferedWriter.write(&quot;\n&quot;);
		bufferedWriter.write(sb.toString());
		bufferedWriter.flush();
		fstream.flush();
		bufferedReader.close();
		bufferedWriter.close();
		fstream.close();
} catch (FileNotFoundException e) {
	e.printStackTrace();
} catch (IOException e) {
	e.printStackTrace();
} catch (Exception e) {
	e.printStackTrace();
} catch (Throwable ex) {
	log.error(&quot;Beanshell failure : &quot;, ex);
	throw ex;
}</stringProp>
